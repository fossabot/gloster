{
  "masterPicture": "./static/logo.png",
  "iconsPath": "/",
  "design": {
    "ios": {
      "pictureAspect": "backgroundAndMargin",
      "backgroundColor": "#338a1e",
      "margin": "14%",
      "assets": {
        "ios6AndPriorIcons": true,
        "ios7AndLaterIcons": true,
        "precomposedIcons": true,
        "declareOnlyDefaultIcon": true
      },
      "appName": "Gloster documentation"
    },
    "desktopBrowser": {
      "design": "raw"
    },
    "windows": {
      "pictureAspect": "noChange",
      "backgroundColor": "#00a300",
      "onConflict": "override",
      "assets": {
        "windows80Ie10Tile": true,
        "windows10Ie11EdgeTiles": {
          "small": true,
          "medium": true,
          "big": true,
          "rectangle": true
        }
      },
      "appName": "Gloster documentation"
    },
    "androidChrome": {
      "pictureAspect": "noChange",
      "themeColor": "#68a70e",
      "manifest": {
        "name": "Gloster documentation",
        "display": "standalone",
        "orientation": "notSet",
        "onConflict": "override",
        "declared": true
      },
      "assets": {
        "legacyIcon": true,
        "lowResolutionIcons": true
      }
    },
    "safariPinnedTab": {
      "pictureAspect": "silhouette",
      "themeColor": "#64cb07"
    }
  },
  "settings": {
    "scalingAlgorithm": "Mitchell",
    "errorOnImageTooSmall": false,
    "readmeFile": false,
    "htmlCodeFile": false,
    "usePathAsIs": false
  }
}
